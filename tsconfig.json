{
	"compilerOptions": {
		"jsx": "react",
		"module": "commonjs",
		"noImplicitAny": true,
		"preserveConstEnums": true,
		"removeComments": true,
		"esModuleInterop": true,
		"outDir": "serverjs",
		"target": "es5",
		"moduleResolution": "node",
		"noEmitOnError": false,
		"strict": false,
		"skipLibCheck": true // skips type checking for node_modules
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
